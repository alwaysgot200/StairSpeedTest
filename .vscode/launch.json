{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug (gdb via MSYS2 bash)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build.debug/stairspeedtest.exe",
      "cwd": "${workspaceFolder}/build.debug",
      "args": ["/web"],
      "MIMode": "gdb",
      "miDebuggerPath": "/mingw64/bin/gdb",
      "miDebuggerArgs": "--interpreter=mi2",
      "launchCompleteCommand": "exec-run",
      "pipeTransport": {
        "pipeProgram": "D:/Programs/scoop/apps/msys2/current/usr/bin/bash.exe",
        "pipeArgs": ["-lc"],
        "debuggerPath": "/mingw64/bin/gdb",
        "pipeCwd": ""
      },
      "environment": [
        { "name": "MSYSTEM", "value": "MINGW64" },
        { "name": "MSYS2_PATH_TYPE", "value": "inherit" },
        {
          "name": "PATH",
          "value": "D:/Programs/scoop/apps/msys2/current/cmakeproxy;D:/Programs/scoop/apps/msys2/current/mingw64/bin;D:/Programs/scoop/apps/msys2/current/usr/bin;D:/Programs/scoop/apps/msys2/current/bin;${env:PATH}"
        }
      ],
      "externalConsole": true,
      "preLaunchTask": "Build (Debug - MSYS2 bash)",
      "stopAtEntry": false,
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "logging": {
        "engineLogging": true,
        "trace": true,
        "traceResponse": true
      }
    }
  ]
}
